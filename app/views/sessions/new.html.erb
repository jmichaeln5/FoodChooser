<div class="card">
  <br>
  <br>

<h1 class="center">Login</h1>

<br>

<%= form_tag '/login' do %>

  <div class="form-group">
    <%= text_field_tag :email, '', placeholder: "Email" %>
  </div>

  <div class="form-group">
    <%= password_field_tag :password, '', placeholder: "Password"%>
  </div>

  <center class="submit">
    <%= submit_tag "Submit" %>
  </center>

<% end %>
<br>
<br>
<br>


</div>
